<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ include file="base.jsp" %>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
  	 <link rel="stylesheet" type="text/css" href="${ basePath}/static/css/bootstrap.min.css" />
    <title>招聘项目的编辑</title>
</head>
<body>
	<div class="container" style="margin-top:20px; background-color:#fff;">
		<div class="row">
			<div class="span12"  >
				<form class="bs-docs-example form-horizontal" style="padding-left:150px;">	
					<div class="control-group">
					  <label class="control-label">招聘岗位名称</label>
					  <div class="controls">
						<input type="text" value="程序员" >
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">开始时间</label>
					  <div class="controls">
						<input id="d422" class="Wdate" type="text" onclick="WdatePicker()" value="2111"/>
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">岗位需求</label>
					  <div class="controls">
						<input type="text" value="会写代码">
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">需求部门</label>
					  <div class="controls">
						<select name="">
						  <option value="">人事部</option>
						  <option>信息部</option>						  
						</select>
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">结束时间</label>
					  <div class="controls">
						<input id="d422" class="Wdate" type="text" onclick="WdatePicker()" value="2102"/>
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">需求数量</label>
					  <div class="controls">
						<input type="text" value="56人" >
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">招聘渠道</label>
					  <div class="controls">
						<select name="">
						  <option value="">人才市场</option>
						  <option>面试</option>						  
						</select>
					  </div>
					</div>
					<div class="control-group">
					  <label class="control-label">备注</label>
					  <div class="controls">
						<textarea rows="3" ></textarea>
					  </div>
					</div>
					<div class="form-actions">
					  <button class="btn btn-primary" type="submit">保存</button>
					  <button class="btn" type="button">返回</button>
					</div>
				</form>
				
			</div>
		</div>
	</div>
</body>
</html>
