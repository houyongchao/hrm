<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ include file="base.jsp" %>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
  	 <link rel="stylesheet" type="text/css" href="${ basePath}/static/css/bootstrap.min.css" />
    <title>显示所有培训机构</title>
</head>
<body>
	<div class="container" style="margin-top:20px; background-color:#fff;">
		<div class="row">
			
			<div class="span12"  >				
				<ul style="margin-top:10px ; margin-bottom:0px;  height:150px;" class="breadcrumb">
					<li class="active" style="font-size:20px;margin-top:10px; ">	
						<form class="form-search">				
							<span style="font-size:16px; margin-right:15px; ">机构名称:</span><input type="text" class="input-medium search-query">
							&nbsp;&nbsp;<span style="font-size:16px; margin-right:15px;">机构地址:</span><input type="text" class="input-medium search-query">
							&nbsp;&nbsp;<span style="font-size:16px; margin-right:15px;">联系人:</span><input type="text" class="input-medium search-query"><br/><br/>
							&nbsp;&nbsp;<span style="font-size:16px; margin-right:15px;">联系人电话:</span><input type="text" class="input-medium search-query">
							&nbsp;&nbsp;<span style="font-size:16px; margin-right:15px;">机构电话:</span><input type="text" class="input-medium search-query">
							<button type="submit" class="btn">Search</button>
						</form>
					</li>
					<li style="float:right;margin-top:10px;" ><a href="#" style="font-size:20px;" class="btn ">添加</a></li>
				</ul>								
				<hr/>
				<table  border="1px;" width="100%" class="table">
					<tr>
						<td>机构名称</td>
						<td>机构地址</td>
						<td>机构电话</td>
						<td>联系人</td>
						<td>联系人电话</td>
						<td>机构备注</td>
						<td colspan="2">相关操作</td>
						
					</tr>
					<tr>
						<td>北大青鸟</td>
						<td>北京</td>
						<td>110</td>
						<td>彭恩静</td>
						<td>120</td>
						<td>一个很不错的培训机构</td>						
						<td colspan="2"><a href="">编辑</a>&nbsp;&nbsp;&nbsp;<a href="">删除</a></td>
						
					</tr>
				</table>				
			</div>
		</div>
	</div>
</body>
</html>
